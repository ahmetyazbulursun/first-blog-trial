@using BlogDeneme.Models.Entity
@model List<Tbl_Galeri>

<div class="instagram-feed-area">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="insta-title">
                    <h5>Galeri</h5>
                </div>
            </div>
        </div>
    </div>
    <!-- Instagram Slides -->
    <div class="instagram-slides owl-carousel">
        <!-- Single Insta Feed -->
        @foreach (var x in Model)
        {
            <div class="single-insta-feed">
                <img src="@x.RESIM" alt="">
                <!-- Hover Effects -->
                <div class="hover-effects">
                    <a href="@x.RESIM" class="d-flex align-items-center justify-content-center"><i class="fa fa-image"></i></a>
                </div>
            </div>
        }
    </div>  
</div>

